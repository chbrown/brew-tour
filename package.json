{
  "name": "brew-tour",
  "version": "0.1.2",
  "description": "GUI and summarizer to help prune excess Homebrew-installed packages",
  "keywords": [
    "homebrew",
    "gui",
    "cleanup"
  ],
  "homepage": "https://github.com/chbrown/brew-tour",
  "repository": {
    "type": "git",
    "url": "git://github.com/chbrown/brew-tour.git"
  },
  "bugs": {
    "email": "Christopher Brown <io@henrian.com>",
    "url": "http://github.com/chbrown/brew-tour/issues"
  },
  "license": "MIT",
  "author": "Christopher Brown <io@henrian.com> (http://henrian.com)",
  "dependencies": {
    "http-enhanced": "~0.5.3",
    "htmlparser2": "*",
    "loge": "~0.0.3",
    "regex-router": "*",
    "send": "*",
    "request": "*",
    "streaming": "*",
    "yargs": "*"
  },
  "bin": {
    "brew-tour": "bin/brew-tour"
  },
  "scripts": {
    "start": "node bin/brew-tour"
  },
  "staticPattern": "static/lib/{file}",
  "staticDependencies": {
    "angular": {
      "filter": [
        "angular.min.js",
        "angular-resource.min.js"
      ]
    },
    "misc-js": {
      "url": "git://github.com/chbrown/misc-js.git",
      "filter": [
        "angular-plugins.js"
      ]
    },
    "ng-storage": "*"
  }
}
